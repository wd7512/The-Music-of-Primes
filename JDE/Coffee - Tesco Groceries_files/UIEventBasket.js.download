bertie.on("UIEventBasket",function(e){try{var UIEventBasketObj={busBasketStateOrderId:e.basket.orderID,busBasketStateTotalQty:e.basket.totalQty,busBasketStateDeliveryPrice:e.basket.deliveryPrice.price,busBasketStateBasketId:e.basket.basketId,busBasketStateGuidePrice:e.basket.guidePrice.price,busBasketItems:e.basket.basketItems,baggingOption:e.basket.bagOption},unitsPerProd=[];e.basket.basketItems.forEach(function(basketItems){var prodQty=basketItems.qty;unitsPerProd.push(prodQty)});var totalItemsCount=unitsPerProd.reduce(function add(a,b){return a+b},0);UIEventBasketObj.busBasketStateUnitsCount=totalItemsCount,gtmDataLayer.set("UIEventBasket",UIEventBasketObj)}catch(e){var errorsObj=gtmDataLayer.get("errors")||{};errorsObj.UIEventBasket="Unavailable",errorsObj.UIEventBasket.eMesssage=e,gtmDataLayer.set("errors",errorsObj)}});