s.domready(function(){
_bt_UA = window._bt_UA || {
   d: {},
   s: function() { return window.JSON.stringify(this.d); },
  ts: function() {
      var now = new Date();
      return now.getTime() + "T" + now.getTimezoneOffset();
    },
  sr: function() {
      return screen.width+"x"+screen.height;
    },
  vp: function() {
      return self.innerWidth+"x"+self.innerHeight;
    },
  cd: function() {
      return screen.colorDepth;
    },
  ce: function() {
      return navigator.cookieEnabled;
    },
  pl: function() {
      var plugins = [];
      try {
        BrightTag.Util.each(navigator.plugins, function(p) {
          plugins.push(p.name);
        });
      } catch (e) {}
      return plugins.join(";");
     },
  ul: function() {
      return navigator.language;
    },
  de: function() {
        return document.charset;
      },
  je: function() {
        return navigator.javaEnabled();
      },
  fl: function() {
        var f = "",
          p = navigator.plugins || null,
          i = 0,
          ii = 10;
        if (p && p.length) {
          for (i; i < p.length; i++) {
            if (p[i].name.indexOf('Shockwave Flash') != -1) {
              f = p[i].description.split('Shockwave Flash ')[1];
              break;
            }
          }
        } else if (window.ActiveXObject) {
          for (ii; ii >= 2; ii--) {
            try {
              var fl = eval("new ActiveXObject('ShockwaveFlash.ShockwaveFlash." + ii + "');");
              if (fl) {
                f = ii + '.0';
                break;
              }
            } catch (e) {}
          }
        }
        return f;
      },
  r: function(attributes) {
    BrightTag.Util.each(attributes, function(a) {
      _bt_UA.d[a] = _bt_UA.d[a] || _bt_UA[a]();
    });
  }
};_bt_UA.r(['ts','sr','vp','cd','ce','pl']);_bt_UA = window._bt_UA || {
   d: {},
   s: function() { return window.JSON.stringify(this.d); },
  ts: function() {
      var now = new Date();
      return now.getTime() + "T" + now.getTimezoneOffset();
    },
  sr: function() {
      return screen.width+"x"+screen.height;
    },
  vp: function() {
      return self.innerWidth+"x"+self.innerHeight;
    },
  cd: function() {
      return screen.colorDepth;
